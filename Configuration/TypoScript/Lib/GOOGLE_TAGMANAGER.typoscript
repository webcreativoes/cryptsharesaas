[frontend.user.isLoggedIn]
   # Falls ein Benutzer eingeloggt ist, wird der Tag Manager deaktiviert
   lib.GOOGLE_TAGMANAGER.value = <!-- Google Tag Manager removed because beUserLogin > 0 ;) -->
[global]

# Tag Manager global deaktivieren (falls gewünscht)
lib.GOOGLE_TAGMANAGER.value =
