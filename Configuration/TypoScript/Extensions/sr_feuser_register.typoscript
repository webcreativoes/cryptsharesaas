    notify.SETFIXED_INVITE = 0
    notify.SETFIXED_APPROVE = 0
    notify.SETFIXED_APPROVE_REVIEW = 0
    notify.SETFIXED_DELETE = 0
    notify.SETFIXED_ENTER = 0
    notify.SETFIXED_ACCEPT = 0
    notify.SETFIXED_REFUSE = 0
