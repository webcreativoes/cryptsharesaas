    fe_userOwnSelf = 1
    fe_userEditSelf = 1
    delete = 1
    pid = 1394
    pidTitleOverride =
    userGroupsPidList = 1396
    registerPID = 1396
    editPID = 1398
    confirmPID =
    confirmInvitationPID =
    confirmType = 0
    loginPID = 1397
    linkToPID =
    linkToPIDAddButton = 0
    module_sys_dmail_category_PIDLIST =
    defaultCODE =
    useShortUrls = 1
    shortUrlLife = 30
    useLocalization = 1
    useLocalCountry = 1
    useLocalLanguage = 1
    onChangeCountryAttribute =
    allowMultipleUserGroupSelection = 0
    salutation = formal
    addAttachment =  {$plugin.tx_srfeuserregister_pi1.enableEmailAttachment}
    addAttachment.cmd = setfixed
    addAttachment.sFK = APPROVE
    addAttachment.file = {$plugin.tx_srfeuserregister_pi1.file.attachmentFile}
    terms.file = EXT:sr_feuser_register/Resources/Public/TextFiles/terms.txt
    terms.url = https://www.cryptshare.express/legal
    displayRequired = TEXT
    displayRequired {
      value = *
      wrap = |
    }
    create = 1
    create {
        preview = 1
        fields = username, password, first_name, last_name, email, telephone, address, house_no, city, static_info_country,language, zip, company, vat, terms_acknowledged, is_trial, disable, module_sys_dmail_html, module_sys_dmail_newsletter
        required = username, password, first_name, last_name, email, telephone, address, house_no, city, static_info_country, zip, company,language, terms_acknowledged
        useEmailAsUsername = 1
        generateUsername = 0
        doNotEnforceUsername = 0
        allowUserGroupSelection = 0
        allowedUserGroups = ALL
        allowedSubgroups =
        deniedUserGroups =
        defaultValues {
            module_sys_dmail_html = 0
            usergroup = 14
            disable = 1
            by_invitation = 0
            language = EN_GB
            static_info_country = GBR
        }
        overrideValues {
            usergroup = 14
            disable = 0
            by_invitation = 0
        }
        evalErrors_evalRuleName_vat = errorMessageForEvalRule
        evalValues {
            password = twice, atLeast[8], atMost[40]
            username = uniqueLocal, atLeast[6], atMost[100]
            email = email,atMost[255],uniqueLocal
            name = atMost[64]
            first_name = atMost[64]
            middle_name = atMost[64]
            last_name = atMost[64]
            date_of_birth = date
            company = atMost[255]
            city = atMost[40]
            zip = atMost[12]
            house_no = atMost[12]
            telephone = atMost[25]
            fax = atMost[25]
            image = upload[0]
            title =
            address =
            www = wwwURL
            comments =
            status =
            module_sys_dmail_category =
            module_sys_dmail_html =
            module_sys_dmail_newsletter =
            by_invitation =
            captcha_response =
        }
    }
    select {
        email {
            module_sys_dmail_category.wrap = | <br/>
        }
    }
    enableEmailConfirmation = 0
    enableAutoLoginOnConfirmation = 0
    enableAutoLoginOnCreate = 14
    enableAdminReview = 0
    autoLoginRedirect_url = /en/sign-up/createcustomeronbwandgetcustomerid
    unsubscribeAllowedFields = module_sys_dmail_newsletter
    authcodeFields.codeLength = 12
    invite < .create
    invite {
        defaultValues {
            by_invitation = 1
        }
        overrideValues {
            by_invitation = 1
        }
    }
    edit = 1
    edit {
        preview = 0
        fields = username, password, first_name, last_name, email, telephone, address, house_no, city, static_info_country, language, zip, company, vat, terms_acknowledged, disable, module_sys_dmail_html, module_sys_dmail_newsletter
        required = first_name, last_name, email, telephone, address, house_no, city, static_info_country, zip, company, language, terms_acknowledged
        useEmailAsUsername = 1
        generateUsername = 0
        doNotEnforceUsername = 0
        allowUserGroupSelection = 0
        allowedUserGroups = ALL
        allowedSubgroups =
        deniedUserGroups =
        defaultValues {
            module_sys_dmail_html = 0
            by_invitation = 0
            language = EN_GB
            static_info_country = GBR
        }
        overrideValues {
            disable = 0
            by_invitation = 0
        }
        evalErrors_evalRuleName_vat = errorMessageForEvalRule
        evalValues {
            password := addToList(unsetEmpty)
            username = uniqueLocal, atLeast[6], atMost[100]
            email = email,atMost[255]
            name = atMost[64]
            first_name = atMost[64]
            middle_name = atMost[64]
            last_name = atMost[64]
            date_of_birth = date
            company = atMost[255]
            city = atMost[40]
            zip = atMost[12]
            house_no = atMost[12]
            telephone = atMost[25]
            fax = atMost[25]
            image = upload[0]
            language =
            title =
            address =
            www = wwwURL
            comments =
            status =
            module_sys_dmail_category =
            module_sys_dmail_html =
            module_sys_dmail_newsletter =
            usergroup =
            by_invitation =
            captcha_response =
        }
    }
    password < .edit
    password {
        preview = 0
        fields = password
        required = password
        defaultValues >
        overrideValues >
        evalValues >
        evalValues {
            password < plugin.tx_srfeuserregister_pi1.edit.evalValues.password
        }
    }
    forceFileDelete = 1
    parseValues {
        username = nospace,trim
        email = trim
        password = trim
        gender = int
        name = trim
        first_name = trim
        middle_name = trim
        last_name = trim
        date_of_birth = date
        address = trim
        title = trim
        company = trim
        vat = trim
        house_no = trim
        city = trim
        country = trim
        static_info_country = trim
        zip = trim
        telephone = trim
        fax = trim
        image = files
        module_sys_dmail_category = multiple
        status = int
        terms_acknowledged = int
        is_trial = int
        www = wwwURL
        comments = trim
    }
    parseFromDBValues {
        date_of_birth = date
    }
    parseToDBValues {
        date_of_birth = date
        image = deleteUnreferencedFiles
    }
    email.from = support@cryptshare.express
    email.fromName = Cryptshare.express
    email.admin = support@cryptshare.express
    email.replyToAdmin = support@cryptshare.express
    email.field = email
    email.replyTo = user
    email.HTMLMail = 0
    email.HTMLMailCSS = EXT:sr_feuser_register/Resources/Public/StyleSheets/htmlmail.css
    email.CREATE_SAVED = 0
